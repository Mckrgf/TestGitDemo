<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android">

    <Row
        android:keyHeight="70dp">
        <Key
            android:codes="10"
            android:keyLabel="1"
            android:keyWidth="25%p"
            />
        <Key
            android:codes="20"
            android:keyLabel="2"
            android:keyWidth="25%p" />
        <Key
            android:codes="30"
            android:keyLabel="3"
            android:keyWidth="25%p" />
        <Key
            android:codes="5"
            android:keyLabel="+"
            android:keyWidth="25%p" />
    </Row>
    <Row
        android:keyHeight="70dp">
        <Key
            android:codes="40"
            android:keyLabel="4"
            android:keyWidth="25%p" />
        <Key
            android:codes="50"
            android:keyLabel="5"
            android:keyWidth="25%p" />
        <Key
            android:codes="60"
            android:keyLabel="6"
            android:keyWidth="25%p" />
        <Key
            android:codes="6"
            android:keyLabel="-"
            android:keyWidth="25%p" />
    </Row>
    <Row
        android:keyHeight="70dp">
        <Key
            android:codes="70"
            android:keyLabel="7"
            android:keyWidth="25%p" />
        <Key
            android:codes="80"
            android:keyLabel="8"
            android:keyWidth="25%p" />
        <Key
            android:codes="90"
            android:keyLabel="9"
            android:keyWidth="25%p" />
        <Key
            android:codes="7"
            android:keyLabel="."
            android:keyWidth="25%p" />
    </Row>
    <Row
        android:keyHeight="70dp">
        <Key
            android:codes="NEXT"
            android:keyEdgeFlags="left"
            android:keyLabel="NEXT"
            android:keyWidth="50%p" />
        <Key
            android:codes="OK"
            android:keyEdgeFlags="right"
            android:keyLabel="OK"
            android:keyOutputText="@string/app_name"
            android:keyWidth="50%p" />
    </Row>


</Keyboard>